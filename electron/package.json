{
    "name": "selene",
    "version": "0.1.0",
    "author": "Duc Nguyen - DukeNgn",
    "description": "Selene IDE - Electron version",
    "main": "scripts/selene.js",
    "theia": {
        "target": "electron",
        "frontend": {
            "config": {
              "applicationName": "Selene IDE",
              "preferences": {
                  "workbench.colorTheme": "One Monokai",
                  "workbench.iconTheme": "material-icon-theme",
                  "workbench.editor.closeOnFileDelete": false
              }
            }
        },
        "generator": {
            "config": {
                "preloadTemplate": "./selene-preload.html"
            }
        }
    },
    "scripts": {
        "preinstall": "node-gyp install",
        "download:plugins": "theia download:plugins -p=true",
        "prepare": "yarn run build && yarn download:plugins",
        "build": "theia build --mode production",
        "start": "theia start --plugins=local-dir:./plugins",
        "package:preview": "electron-builder --dir",
        "package": "electron-builder"
    },
    "dependencies": {
        "@theia/callhierarchy": "latest",
        "@theia/console": "latest",
        "@theia/core": "latest",
        "@theia/debug": "latest",
        "@theia/editor": "latest",
        "@theia/editor-preview": "latest",
        "@theia/file-search": "latest",
        "@theia/filesystem": "latest",
        "@theia/getting-started": "latest",
        "@theia/git": "latest",
        "@theia/keymaps": "latest",
        "@theia/markers": "latest",
